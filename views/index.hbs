

<section class="get">
    <h3>Get Data</h3>
    <a href="/get-data">LOAD DATA</a>
    <div> 
        {{# each items }}
            <article class="item">
                <div>code: {{ this.code }}</div>
                <div>name: {{ this.name }}</div>
                <div>ID: {{ this._id }}</div>
            </article>
        {{/each}}
    </div>
</section>